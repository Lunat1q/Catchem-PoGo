<UserControl xmlns:controllers="clr-namespace:Catchem.Controllers"  x:Class="Catchem.Pages.SettingsPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             Width="890.487"
             Height="2107.5"
             mc:Ignorable="d">
    <Grid Background="#FFF9F9F9">
        <ScrollViewer Margin="0,0,0,10">
            <Grid x:Name="SettingsGrid">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="300*" />
                    <ColumnDefinition Width="300*" />
                </Grid.ColumnDefinitions>

                <Grid Grid.Column="1" Margin="2,0,5,0" VerticalAlignment="Top">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>
                    <GroupBox x:Name="ProxyGroupBox"
            			Height="147"
            			Header="Proxy"
            			Tag="%PROXY_WORK%" Margin="0,5,0,0">
                        <Grid Margin="0,0,0,0" IsEnabled="True">
                            <CheckBox x:Name="c_UseProxy"
            					Width="101"
            					Height="21"
            					Margin="10,6,0,0"
            					HorizontalAlignment="Left"
            					VerticalAlignment="Top"
            					Checked="BotPropertyChanged"
            					Content="UseProxy"
            					Unchecked="BotPropertyChanged" Tag="%USE_PROXY%" />
                            <TextBlock x:Name="c_ProxyLoginLabel"
            					Width="79"
            					Margin="10,31,0,0"
            					HorizontalAlignment="Left"
            					VerticalAlignment="Top"
            					Text="Login" Tag="%LOGIN%" />
                            <TextBox x:Name="c_ProxyLogin"
            					Height="34"
            					Margin="145,22,125,0"
            					VerticalAlignment="Top"
            					Foreground="#FF56C2B0"
            					Template="{StaticResource Pogo}"
            					Text="Username"
            					TextChanged="BotPropertyChanged"
            					TextWrapping="Wrap" />
                            <TextBlock x:Name="c_ProxyPassLabel"
            					Width="79"
            					Margin="10,62,0,0"
            					HorizontalAlignment="Left"
            					VerticalAlignment="Top"
            					Text="Password" Tag="%PASSWORD%" />
                            <PasswordBox x:Name="c_ProxyPass"
            					Height="28"
            					Margin="145,59,10,0"
            					VerticalAlignment="Top"
            					Foreground="#FF56C2B0"
            					Password="password"
            					PasswordChanged="BotPropertyChanged"
            					Template="{StaticResource PogoPassword}" />
                            <TextBlock x:Name="c_ProxyUriLabel"
            					Width="79"
            					Margin="10,98,0,0"
            					HorizontalAlignment="Left"
            					VerticalAlignment="Top"
            					Text="Host:port"
            					RenderTransformOrigin="0.494,0.5" Tag="%HOST_PORT%" />
                            <TextBox x:Name="c_ProxyUri"
            					Height="29"
            					Margin="145,92,10,0"
            					VerticalAlignment="Top"
            					Foreground="#FF56C2B0"
            					Template="{StaticResource Pogo}"
            					TextChanged="BotPropertyChanged"
            					TextWrapping="Wrap" />
                            <Button x:Name="btn_textProxy"
            					Width="110"
            					Height="29"
            					Margin="0,25,10,0"
            					HorizontalAlignment="Right"
            					VerticalAlignment="Top"
            					Click="btn_textProxy_Click"
            					Content="TEST PROXY"
            					FontFamily="BOLD"
            					FontWeight="Bold"
            					Style="{StaticResource ResourceKey=Flatbutton}"
            					TextOptions.TextFormattingMode="Display" Background="{DynamicResource NormalButtonColor}" Tag="%TEST_PROXY%"/>
                        </Grid>
                    </GroupBox>
                    <GroupBox x:Name="NavigationGroupBox"
            			Height="527"
            			Margin="0,5,0,0"
            			VerticalAlignment="Top"
            			Header="Navigation" Tag="%NAVIGATION_BOX%" Grid.Row="1">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>
                            <TextBlock x:Name="c_DefaultAltitudeLabel"
                    			Margin="10,18,0,0"
                    			VerticalAlignment="Top"
                    			Text="Altitude Current"
                    			RenderTransformOrigin="0.5,0.423" Tag="%ALTITUDE_CURRENT%" />
                            <TextBox x:Name="c_DefaultAltitude"
                    			Grid.Column="1"
                    			Height="29"
                    			Margin="15,13,10,0"
                    			VerticalAlignment="Top"
                    			Template="{StaticResource Pogo}"
                    			TextChanged="BotPropertyChanged"
                    			TextWrapping="Wrap" />
                            <TextBlock Grid.Column="0" x:Name="c_DefaultAltitudeMinLabel"
                    			Margin="10,52,0,0"
                    			VerticalAlignment="Top"
                    			Text="Altitude Min"
                    			RenderTransformOrigin="0.5,0.423" Tag="%ALTITUDE_MIN%" />
                            <TextBox x:Name="c_DefaultAltitudeMin"
                    			Grid.Column="1"
                    			Height="29"
                    			Margin="15,47,10,0"
                    			VerticalAlignment="Top"
                    			Template="{StaticResource Pogo}"
                    			TextChanged="BotPropertyChanged"
                    			TextWrapping="Wrap" />
                            <TextBlock Grid.Column="0" x:Name="c_DefaultAltitudeMaxLabel"
                    			Margin="10,86,0,0"
                    			VerticalAlignment="Top"
                    			Text="Altitude Max"
                    			RenderTransformOrigin="0.5,0.423" Tag="%ALTITUDE_MAX%" />
                            <TextBox x:Name="c_DefaultAltitudeMax"
                    			Grid.Column="1"
                    			Height="29"
                    			Margin="15,81,10,0"
                    			VerticalAlignment="Top"
                    			Template="{StaticResource Pogo}"
                    			TextChanged="BotPropertyChanged"
                    			TextWrapping="Wrap" />
                            <TextBlock Grid.Column="0" x:Name="c_DefaultLatitudeLabel"
                    			Margin="10,120,0,0"
                    			VerticalAlignment="Top"
                    			Text="Latitude"
                    			RenderTransformOrigin="0.5,0.423" Tag="%LATITUDE%" />
                            <TextBox x:Name="c_DefaultLatitude"
                    			Grid.Column="1"
                    			Height="29"
                    			Margin="15,115,10,0"
                    			VerticalAlignment="Top"
                    			Template="{StaticResource Pogo}"
                    			TextChanged="BotPropertyChanged"
                    			TextWrapping="Wrap" />
                            <TextBlock Grid.Column="0" x:Name="c_DefaultLongitudeLabel"
                    			Margin="10,154,0,0"
                    			VerticalAlignment="Top"
                    			Text="Longitude"
                    			RenderTransformOrigin="0.5,0.423" Tag="%LONGITUDE%" />
                            <TextBox x:Name="c_DefaultLongitude"
                    			Grid.Column="1"
                    			Height="29"
                    			Margin="15,149,10,0"
                    			VerticalAlignment="Top"
                    			Template="{StaticResource Pogo}"
                    			TextChanged="BotPropertyChanged"
                    			TextWrapping="Wrap" />

                            <CheckBox x:Name="c_Teleport"
                    			Height="16"
                    			Margin="10,183,10,0"
                    			VerticalAlignment="Top"
                    			Checked="BotPropertyChanged"
                    			Content="Teleport (High ban chance)"
                    			Unchecked="BotPropertyChanged" Tag="%TELEPORT%" Grid.ColumnSpan="2" Grid.Column="0" />
                            <TextBlock Grid.Column="0" x:Name="c_MaxSpawnLocationOffsetLabel"
                    			Margin="10,209,0,0"
                    			VerticalAlignment="Top"
                    			Text="Max Spawn Location Offset"
                    			RenderTransformOrigin="0.5,0.423" Tag="%MAX_SPAWN_LOCATION_OFFSET%" />

                            <TextBox x:Name="c_MaxSpawnLocationOffset"
                    			Grid.Column="1"
                    			Height="29"
                    			Margin="15,204,10,0"
                    			VerticalAlignment="Top"
                    			Template="{StaticResource Pogo}"
                    			TextChanged="BotPropertyChanged"
                    			TextWrapping="Wrap" />
                            <TextBlock Grid.Column="0" x:Name="c_MaxTravelDistanceInMetersLabel"
                    			Margin="10,243,0,0"
                    			VerticalAlignment="Top"
                    			Text="Max Travel Distance In Meters"
                    			RenderTransformOrigin="0.5,0.423" Tag="%MAX_TRAVEL_DISTANCE_IN_METERS%" />
                            <TextBox x:Name="c_MaxTravelDistanceInMeters"
                    			Grid.Column="1"
                    			Height="29"
                    			Margin="15,238,10,0"
                    			VerticalAlignment="Top"
                    			Template="{StaticResource Pogo}"
                    			TextChanged="BotPropertyChanged"
                    			TextWrapping="Wrap" />
                            <CheckBox x:Name="c_UseDiscoveryPathing"
                    			Height="16"
                    			Margin="10,269,0,0"
                    			VerticalAlignment="Top"
                    			Checked="BotPropertyChanged"
                    			Content="Use Discovery Pathing"
                    			Unchecked="BotPropertyChanged" Tag="%USE_DISCOVERY_PATHING%" Grid.ColumnSpan="2" Grid.Column="0" />
                            <TextBlock Grid.Column="0" x:Name="c_WalkingSpeedMinLabel"
                    			Margin="10,295,0,0"
                    			VerticalAlignment="Top"
                    			Text="Walking Speed Min (km/h)"
                    			RenderTransformOrigin="0.5,0.423" Tag="%WALKING_SPEED_MIN%" />
                            <TextBox x:Name="c_WalkingSpeedMin"
                    			Grid.Column="1"
                    			Height="29"
                    			Margin="15,290,10,0"
                    			VerticalAlignment="Top"
                    			Template="{StaticResource Pogo}"
                    			TextChanged="BotPropertyChanged"
                    			TextWrapping="Wrap" />
                            <TextBlock Grid.Column="0" x:Name="c_WalkingSpeedMaxLabel"
                    			Margin="10,329,0,0"
                    			VerticalAlignment="Top"
                    			Text="Walking Speed Max (km/h)"
                    			RenderTransformOrigin="0.5,0.423" Tag="%WALKING_SPEED_MAX%" />
                            <TextBox x:Name="c_WalkingSpeedMax"
                    			Grid.Column="1"
                    			Height="29"
                    			Margin="15,324,10,0"
                    			VerticalAlignment="Top"
                    			Template="{StaticResource Pogo}"
                    			TextChanged="BotPropertyChanged"
                    			TextWrapping="Wrap" />
                            <CheckBox x:Name="c_UseHumanPathing"
                    			Height="16"
                    			Margin="10,358,0,0"
                    			VerticalAlignment="Top"
                    			Checked="BotPropertyChanged"
                    			Content="Use Human Pathing"
                    			Unchecked="BotPropertyChanged" Grid.ColumnSpan="2" Grid.Column="0" Tag="%USE_HUMAN_PATHING%" />
                            <CheckBox x:Name="c_StopBotToAvoidBanOnUnknownLoginError"
                    			Height="16"
                    			Margin="10,379,0,0"
                    			VerticalAlignment="Top"
                    			Checked="BotPropertyChanged"
                    			Content="Stop Bot To Avoid Ban On Unknown Login Error"
                    			Unchecked="BotPropertyChanged" Grid.ColumnSpan="2" Grid.Column="0" Tag="%STOP_BOT_TO_AVOID_BAN%" />
                            <CheckBox x:Name="c_UseMapzenApiElevation"
                    			Height="16"
                    			Margin="10,400,0,0"
                    			VerticalAlignment="Top"
                    			Checked="BotPropertyChanged"
                    			Content="Use Mapzen Elevation API"
                    			Unchecked="BotPropertyChanged" Grid.ColumnSpan="2" Grid.Column="0" Tag="%USE_MAPZEN_ELEVATION_API%" />
                            <TextBlock Grid.Column="0" x:Name="c_RoutingServiceLabel"
                    			Margin="10,426,0,0"
                    			VerticalAlignment="Top"
                    			Text="Routing service"
                    			RenderTransformOrigin="0.5,0.423" Tag="%ROUTING_SERVICE%" />
                            <ComboBox x:Name="c_RoutingService"
                    			Height="24"
                    			Margin="15,423,10,0"  
                    			VerticalAlignment="Top" Grid.Column="1"  
                    			SelectionChanged="BotPropertyChanged" />
                            <TextBlock Grid.Column="0" x:Name="CustomRouteLabel"
                    			Margin="10,478,0,0"
                    			VerticalAlignment="Top"
                    			Text="Select Custom Route"
                    			RenderTransformOrigin="0.5,0.423" Tag="%SELECT_CUSTOM_PATH%" />
                            <ComboBox x:Name="CustomRouteComboBox"
                    			Height="24"
                    			Margin="15,473,10,0"  
                    			VerticalAlignment="Top" Grid.Column="1"  
                    			SelectionChanged="CustomRouteComboBox_SelectionChanged" />
                            <CheckBox x:Name="c_UseCustomRoute"
                    			Height="16"
                    			Margin="10,452,0,0"
                    			VerticalAlignment="Top"
                    			Checked="BotPropertyChanged"
                    			Content="Use Custom Route"
                    			Unchecked="BotPropertyChanged" Grid.ColumnSpan="2" Grid.Column="0" Tag="%USE_CUSTOM_PATH%" />

                        </Grid>
                    </GroupBox>

                    <GroupBox x:Name="DeviceInfoGroupBox"
            			Height="508"
            			Margin="0,5,0,0"
            			VerticalAlignment="Top"
            			Header="Device Info" Tag="%DEVICE_INFO_BOX%" Grid.Row="3">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>
                            <TextBlock x:Name="c_DeviceIdLabel"
            					Margin="10,18,0,0"
            					VerticalAlignment="Top"
            					Text="Device Id"
            					RenderTransformOrigin="0.5,0.423" Tag="%DEVICE_ID%" />
                            <TextBox x:Name="c_DeviceId"
            					Grid.Column="1"
            					Height="29"
            					Margin="10,13,77,0"
            					VerticalAlignment="Top"
            					Template="{StaticResource Pogo}"
            					TextChanged="BotPropertyChanged"
            					TextWrapping="Wrap" />
                            <TextBlock Grid.Column="0" x:Name="c_AndroidBoardNameLabel"
            					Margin="10,52,0,0"
            					VerticalAlignment="Top"
            					Text="Android Board Name"
            					RenderTransformOrigin="0.5,0.423" Tag="%ANDROID_BOARD_NAME%" />
                            <TextBox x:Name="c_AndroidBoardName"
            					Grid.Column="1"
            					Height="29"
            					Margin="10,47,10,0"
            					VerticalAlignment="Top"
            					Template="{StaticResource Pogo}"
            					TextChanged="BotPropertyChanged"
            					TextWrapping="Wrap" />
                            <TextBlock Grid.Column="0" x:Name="c_AndroidBootLoaderLabel"
            					Margin="10,86,0,0"
            					VerticalAlignment="Top"
            					Text="Android Bootloader"
            					RenderTransformOrigin="0.5,0.423" Tag="%ANDROID_BOOTLOADER%" />
                            <TextBox x:Name="c_AndroidBootLoader"
            					Grid.Column="1"
            					Height="29"
            					Margin="10,81,10,0"
            					VerticalAlignment="Top"
            					Template="{StaticResource Pogo}"
            					TextChanged="BotPropertyChanged"
            					TextWrapping="Wrap" />
                            <TextBlock Grid.Column="0" x:Name="c_DeviceBrandLabel"
            					Margin="10,120,0,0"
            					VerticalAlignment="Top"
            					Text="Device Brand"
            					RenderTransformOrigin="0.5,0.423" Tag="%DEVICE_BRAND%" />
                            <TextBox x:Name="c_DeviceBrand"
            					Grid.Column="1"
            					Height="29"
            					Margin="10,115,10,0"
            					VerticalAlignment="Top"
            					Template="{StaticResource Pogo}"
            					TextChanged="BotPropertyChanged"
            					TextWrapping="Wrap" />
                            <TextBlock Grid.Column="0" x:Name="c_DeviceModelLabel"
            					Margin="10,154,0,0"
            					VerticalAlignment="Top"
            					Text="Device Model"
            					RenderTransformOrigin="0.5,0.423" Tag="%DEVICE_MODEL%" />
                            <TextBox x:Name="c_DeviceModel"
            					Grid.Column="1"
            					Height="29"
            					Margin="10,149,10,0"
            					VerticalAlignment="Top"
            					Template="{StaticResource Pogo}"
            					TextChanged="BotPropertyChanged"
            					TextWrapping="Wrap" />
                            <TextBlock Grid.Column="0" x:Name="c_DeviceModelIdentifierLabel"
            					Margin="10,188,0,0"
            					VerticalAlignment="Top"
            					Text="Device Model Identifier"
            					RenderTransformOrigin="0.5,0.423" Tag="%DEVICE_MODEL_IDENTIFIER%" />
                            <TextBox x:Name="c_DeviceModelIdentifier"
            					Grid.Column="1"
            					Height="29"
            					Margin="10,183,10,0"
            					VerticalAlignment="Top"
            					Template="{StaticResource Pogo}"
            					TextChanged="BotPropertyChanged"
            					TextWrapping="Wrap" />
                            <TextBlock Grid.Column="0" x:Name="c_DeviceModelBootLabel"
            					Margin="10,222,0,0"
            					VerticalAlignment="Top"
            					Text="Device Model Boot"
            					RenderTransformOrigin="0.5,0.423" Tag="%DEVICE_MODEL_BOOT%" />
                            <TextBox x:Name="c_DeviceModelBoot"
            					Grid.Column="1"
            					Height="29"
            					Margin="10,217,10,0"
            					VerticalAlignment="Top"
            					Template="{StaticResource Pogo}"
            					TextChanged="BotPropertyChanged"
            					TextWrapping="Wrap" />
                            <TextBlock Grid.Column="0" x:Name="c_HardwareManufacturerLabel"
            					Margin="10,256,0,0"
            					VerticalAlignment="Top"
            					Text="Hardware Manufacturer"
            					RenderTransformOrigin="0.5,0.423" Tag="%HARDWARE_MANUFACTURER%" />
                            <TextBox x:Name="c_HardwareManufacturer"
            					Grid.Column="1"
            					Height="29"
            					Margin="10,251,10,0"
            					VerticalAlignment="Top"
            					Template="{StaticResource Pogo}"
            					TextChanged="BotPropertyChanged"
            					TextWrapping="Wrap" />
                            <TextBlock Grid.Column="0" x:Name="c_HardWareModelLabel"
            					Margin="10,290,0,0"
            					VerticalAlignment="Top"
            					Text="Hardware Model"
            					RenderTransformOrigin="0.5,0.423" Tag="%HARDWARE_MODEL%" />
                            <TextBox x:Name="c_HardWareModel"
            					Grid.Column="1"
            					Height="29"
            					Margin="10,285,10,0"
            					VerticalAlignment="Top"
            					Template="{StaticResource Pogo}"
            					TextChanged="BotPropertyChanged"
            					TextWrapping="Wrap" />
                            <TextBlock Grid.Column="0" x:Name="c_FirmwareBrandLabel"
            					Margin="10,324,0,0"
            					VerticalAlignment="Top"
            					Text="Firmware Brand"
            					RenderTransformOrigin="0.5,0.423" Tag="%FIRMWARE_BRAND%" />
                            <TextBox x:Name="c_FirmwareBrand"
            					Grid.Column="1"
            					Height="29"
            					Margin="10,319,10,0"
            					VerticalAlignment="Top"
            					Template="{StaticResource Pogo}"
            					TextChanged="BotPropertyChanged"
            					TextWrapping="Wrap" />
                            <TextBlock Grid.Column="0" x:Name="c_FirmwareTagsLabel"
            					Margin="10,358,0,0"
            					VerticalAlignment="Top"
            					Text="Firmware Tags"
            					RenderTransformOrigin="0.5,0.423" Tag="%FIRMWARE_TAGS%" />
                            <TextBox x:Name="c_FirmwareTags"
            					Grid.Column="1"
            					Height="29"
            					Margin="10,353,10,0"
            					VerticalAlignment="Top"
            					Template="{StaticResource Pogo}"
            					TextChanged="BotPropertyChanged"
            					TextWrapping="Wrap" />
                            <TextBlock Grid.Column="0" x:Name="c_FirmwareTypeLabel"
            					Margin="10,392,0,0"
            					VerticalAlignment="Top"
            					Text="Firmware Type"
            					RenderTransformOrigin="0.5,0.423" Tag="%FIRMWARE_TYPE%" />
                            <TextBox x:Name="c_FirmwareType"
            					Grid.Column="1"
            					Height="29"
            					Margin="10,387,10,0"
            					VerticalAlignment="Top"
            					Template="{StaticResource Pogo}"
            					TextChanged="BotPropertyChanged"
            					TextWrapping="Wrap" />
                            <TextBlock Grid.Column="0" x:Name="c_FirmwareFingerprintLabel"
            					Margin="10,426,0,0"
            					VerticalAlignment="Top"
            					Text="Firmware Fingerprint"
            					RenderTransformOrigin="0.5,0.423" Tag="%FIRMWARE_FINGERPRINT%" />
                            <TextBox x:Name="c_FirmwareFingerprint"
            					Grid.Column="1"
            					Height="29"
            					Margin="10,421,10,0"
            					VerticalAlignment="Top"
            					Template="{StaticResource Pogo}"
            					TextChanged="BotPropertyChanged"
            					TextWrapping="Wrap" />
                            <Button x:Name="b_getDataFromRealPhone"
            					Height="29"
            					Margin="10,455,10,0"
            					VerticalAlignment="Top"
            					Click="b_getDataFromRealPhone_Click"
            					Content="Get Data from Your Android (DISABLED)"
            					FontFamily="BOLD"
            					FontWeight="Bold"
            					Style="{StaticResource ResourceKey=Flatbutton}"
            					TextOptions.TextFormattingMode="Display" Width="Auto" Grid.Column="1" Tag="%GET_DATA_FROM_YOUR_ANDROID%" IsEnabled="False">
                                <Button.Background>
                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                        <LinearGradientBrush.RelativeTransform>
                                            <TransformGroup>
                                                <ScaleTransform CenterY="0.5" CenterX="0.5"/>
                                                <SkewTransform CenterY="0.5" CenterX="0.5"/>
                                                <RotateTransform Angle="90" CenterY="0.5" CenterX="0.5"/>
                                                <TranslateTransform/>
                                            </TransformGroup>
                                        </LinearGradientBrush.RelativeTransform>
                                        <GradientStop Color="LightGray" Offset="0"/>
                                        <GradientStop Color="#FF898989" Offset="1"/>
                                    </LinearGradientBrush>
                                </Button.Background>
                            </Button>
                            <Button x:Name="b_generateRandomDeviceId"
            					Grid.Column="1"
            					Width="62"
            					Height="29"
            					Margin="0,12,10,0"
            					HorizontalAlignment="Right"
            					VerticalAlignment="Top"
            					Click="b_generateRandomDeviceId_Click"
            					Content="Random"
            					FontFamily="BOLD"
            					FontWeight="Bold"
            					Style="{StaticResource ResourceKey=Flatbutton}"
            					TextOptions.TextFormattingMode="Display" Background="{DynamicResource NormalButtonColor}" Tag="%RANDOM_DEVICE_ID%"/>
                            <Button Grid.Column="0" x:Name="b_GenerateNewAndroid"
            					Height="29"
            					Margin="10,455,0,0"
            					VerticalAlignment="Top"
            					Content="Create new"
            					FontFamily="BOLD"
            					FontWeight="Bold"
            					Style="{StaticResource ResourceKey=Flatbutton}"
            					TextOptions.TextFormattingMode="Display" Width="Auto" Click="b_GenerateNewAndroid_Click" Background="{DynamicResource NormalButtonColor}" Tag="%CREATE_NEW%"/>
                        </Grid>
                    </GroupBox>

                    <GroupBox x:Name="DelaysGroupBox"
            			Height="624"
            			Margin="0,5,0,0"
            			VerticalAlignment="Top"
            			Header="Delays"
            			RenderTransformOrigin="0.479,0.565" Tag="%DELAYS_BOX%" Grid.Row="4">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>
                            <TextBlock x:Name="c_DelayBetweenPlayerActionsLabel"
                    			Margin="10,13,0,0"
                    			VerticalAlignment="Top"
                    			Text="Delay Between Player Actions" Tag="%DELAY_BETWEEN_PLAYER_ACTIONS%" />
                            <TextBox x:Name="c_DelayBetweenPlayerActions"
                    			Grid.Column="1"
                    			Height="29"
                    			Margin="15,7,10,0"
                    			VerticalAlignment="Top"
                    			Template="{StaticResource Pogo}"
                    			Text="0"
                    			TextChanged="BotPropertyChanged"
                    			TextWrapping="Wrap" />
                            <TextBlock Grid.Column="0" x:Name="c_DelayPositionCheckStateLabel"
                    			Margin="10,47,0,0"
                    			VerticalAlignment="Top"
                    			Text="Delay Position Check State" Tag="%DELAY_POSTION_CHECK_STATE%" />
                            <TextBox x:Name="c_DelayPositionCheckState"
                    			Grid.Column="1"
                    			Height="29"
                    			Margin="15,41,10,0"
                    			VerticalAlignment="Top"
                    			Template="{StaticResource Pogo}"
                    			Text="0"
                    			TextChanged="BotPropertyChanged"
                    			TextWrapping="Wrap" />
                            <TextBlock Grid.Column="0" x:Name="c_DelayPokestopLabel"
                    			Margin="10,81,0,0"
                    			VerticalAlignment="Top"
                    			Text="Delay Pokestop" Tag="%DELAY_POKESTOP%" />
                            <TextBox x:Name="c_DelayPokestop"
                    			Grid.Column="1"
                    			Height="29"
                    			Margin="15,75,10,0"
                    			VerticalAlignment="Top"
                    			Template="{StaticResource Pogo}"
                    			Text="0"
