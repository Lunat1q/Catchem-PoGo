<UserControl x:Class="Catchem.Pages.RouteCreatorPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      mc:Ignorable="d" d:DesignHeight="600" d:DesignWidth="760">

    <Grid x:Name="RouteCreatorGrid">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="35"/>
            <RowDefinition Height="50"/>
            <RowDefinition Height="495"/>
            <RowDefinition Height="20"/>
        </Grid.RowDefinitions>
        <Rectangle Fill="#FFF4F4F5" StrokeThickness="0" Grid.RowSpan="4" Grid.ColumnSpan="2" Height="600" VerticalAlignment="Top" Margin="0,0,0,-6"/>
        <Rectangle Fill="#0AFF3C" HorizontalAlignment="Left" Height="42" Margin="0,1,0,-22" Grid.Row="3" StrokeThickness="0" VerticalAlignment="Top" Width="760" Grid.ColumnSpan="2"/>

    </Grid>
</UserControl>
