                                Height="29"
                                Margin="94,92,125,0"
                                VerticalAlignment="Top"
                                Foreground="#FF56C2B0"
                                Template="{StaticResource pogo}"
                                TextChanged="BotPropertyChanged"
                                TextWrapping="Wrap" />
