                                Width="79"
                                Margin="10,27,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Content="Login" />
                        <TextBox x:Name="c_ProxyLogin"
                                Height="34"
                                Margin="94,22,10,0"
                                VerticalAlignment="Top"
                                Foreground="#FF56C2B0"
                                Template="{StaticResource pogo}"
                                Text="Username"
                                TextChanged="BotPropertyChanged"
                                TextWrapping="Wrap" />
                        <Label x:Name="c_ProxyPassLabel"
                                Width="79"
                                Margin="10,59,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Content="Password" />
                        <PasswordBox x:Name="c_ProxyPass"
                                Height="28"
                                Margin="94,59,10,0"
                                VerticalAlignment="Top"
                                Foreground="#FF56C2B0"
