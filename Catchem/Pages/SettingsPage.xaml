<UserControl x:Class="Catchem.Pages.SettingsPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Catchem"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             Width="890.487"
             Height="2107.5"
             mc:Ignorable="d">
    <Grid Background="#FFF9F9F9">
        <ScrollViewer Grid.ColumnSpan="2">
            <Grid x:Name="settings_grid">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="300*" />
                    <ColumnDefinition Width="300*" />
                </Grid.ColumnDefinitions>

                <GroupBox x:Name="authGroupBox"
                        Height="222"
                        VerticalAlignment="Top"
                        Header="Authentication">
                    <Grid Margin="0,0,0,0">
                        <Label x:Name="authBoxLabel"
                                Width="80"
                                Margin="10,17,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Content="Auth type" />
                        <ComboBox x:Name="authBox"
                                Height="24"
                                Margin="95,17,84,0"
                                VerticalAlignment="Top"
                                SelectionChanged="authBox_SelectionChanged"/>
                        <Label x:Name="loginBoxLabel"
                                Width="80"
                                Margin="10,52,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Content="Login"/>
                        <TextBox x:Name="loginBox"
                                Height="32"
                                Margin="95,50,10,0"
                                VerticalAlignment="Top"
                                Foreground="#FF56C2B0"
                                Template="{StaticResource pogo}"
                                Text="Username"
                                TextChanged="loginBox_TextChanged"
                                TextWrapping="Wrap"/>
                        <Label x:Name="passwordBoxLabel"
                                Width="80"
                                Margin="10,89,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Content="Password" />
                        <PasswordBox x:Name="passwordBox"
                                Height="28"
                                Margin="95,87,10,0"
                                VerticalAlignment="Top"
                                Foreground="#FF56C2B0"
                                Password="password"
                                PasswordChanged="passwordBox_PasswordChanged"
                                Template="{StaticResource pogo_password}" />
                        <CheckBox x:Name="c_AutoStartThisProfile"
                                Margin="0,23,10,0"
                                HorizontalAlignment="Right"
                                VerticalAlignment="Top"
                                Checked="BotPropertyChanged"
                                Content="Autostart"
                                Unchecked="BotPropertyChanged"/>
                        <CheckBox x:Name="c_AutoCompleteTutorial"
                        	Margin="42,127,0,0"
                        	HorizontalAlignment="Left"
                        	VerticalAlignment="Top"
                        	Checked="BotPropertyChanged"
                        	Content="Auto Complete Tutorial and set name as below"
                        	Unchecked="BotPropertyChanged" FontSize="14.667" />
                        <Label x:Name="c_EvolveAboveIvValueLabel_Copy"
                        	Width="80"
                        	Margin="10,161,0,0"
                        	HorizontalAlignment="Left"
                        	VerticalAlignment="Top"
                        	Content="Desired nick" />
                        <TextBox x:Name="c_DesiredNickname"
                        	Height="29"
                        	Margin="95,161,10,0"
                        	VerticalAlignment="Top"
                        	Template="{StaticResource pogo}"
                        	Text="CatchemFan"
                        	TextChanged="BotPropertyChanged"
                        	TextWrapping="Wrap" />
                    </Grid>
                </GroupBox>

                <GroupBox x:Name="proxyGroupBox"
                        Grid.Column="1"
                        Height="147"
                        VerticalAlignment="Top"
                        Header="Proxy"
                        Margin="5,0,0,0">
                    <Grid Margin="0,0,0,0" IsEnabled="True">
